
public class ThirdPattern {

	public static void main(String[] args) {
		int numberofrows = 4;
		int spaces = 4;
		for(int i =0;i<numberofrows;i++) {
			for(int j=1;j<=spaces;j++) {
				System.out.print(" ");
			}
		
		for(int i=0;i<=4;i++) {
			int z =(int)Math.pow(11,i);
			System.out.println(z);
		}
		
		}
	}

}
