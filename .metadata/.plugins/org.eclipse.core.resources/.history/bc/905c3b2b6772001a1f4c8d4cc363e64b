
public class ThirdPattern {

	public static void main(String[] args) {
		/*int numberofrows = 4;
		int spaces = 4;
		for(int i =0;i<numberofrows;i++) {
			for(int j=1;j<=spaces;j++) {
				System.out.print(" ");
			}
		
		for(int k=0;k<=i;k++) {
			int z =(int)Math.pow(11,i);
			System.out.print(z);
		}
		spaces--;
		System.out.println();
		
		}*/
		int numberofrows = 5;
		for (int i = 0; i < numberofrows; i++) {
			for (int j = numberofrows - i; j > 1; j--) {
				System.out.print(" ");
			}

			for (int j = 0; j <i; j++) {
				int z =(int)Math.pow(11,i);
				System.out.print(z);
			}
			System.out.println();
		}
	}

}
