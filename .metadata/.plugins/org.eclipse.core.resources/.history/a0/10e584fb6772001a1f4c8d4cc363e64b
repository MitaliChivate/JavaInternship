
public class ThirdPattern {

	public static void main(String[] args) {
	/*	for(int i=0;i<=4;i++) {
			int z =(int)Math.pow(11,i);
			System.out.println(z);
		}*/
		int numberofrows = 5;
		for (int i = 0; i < numberofrows; i++) {
			for (int j = numberofrows - i; j > 1; j--) {
				System.out.print(" ");
				int z =(int)Math.pow(11,i);
				System.out.print(z);
			}

			
				
				System.out.println();
	
		}
		
		
	}

}
