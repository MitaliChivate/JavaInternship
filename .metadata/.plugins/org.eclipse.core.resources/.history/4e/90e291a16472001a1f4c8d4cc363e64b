
public class ThirdPattern {

	public static void main(String[] args) {
		int numberofrows=4;
		for(int i=0;i<=4;i++) {
			for (int j = numberofrows - i; j > 1; j--) {
				System.out.print(" ");
			}

			for(int i=0;i<=4;i++) {
			int z =(int)Math.pow(11,i);
			System.out.println(z);
			}
		}
		

	}

}
